from sys import stdin

def sortZeroesAndOne(arr,n):
    index=0
    indexR=n-1
    while(indexR>index):
        if arr[index]==1:
            arr[indexR],arr[index]=arr[index],arr[indexR]
            indexR=indexR-1
        else:
            index=index+1
            
    return arr
#Taking Input Using Fast I/O
def takeInput() :
    n = int(stdin.readline().strip())

    if n == 0 :
        return list(), 0

    
    arr = list(map(int, stdin.readline().strip().split(" ")))
    return arr, n
def printList(arr, n) :
    for i in range(n) :
        print(arr[i], end = ' ')
    print()
    
t = int(stdin.readline().strip())

while t > 0 :
    
    arr, n = takeInput()
    sortZeroesAndOne(arr, n)
    printList(arr, n)
    print()

    t -= 1
